                x-transition:enter="transition ease-out duration-1000"
                x-transition:enter-start="scale-50 opacity-100"
                x-transition:enter-end="scale-100 opacity-0"
                class="absolute z-20 flex items-center justify-center w-24 h-24 rounded-full opacity-0 bg-gradient-to-b from-vanta-500 hover:to-vanta-700 to-vanta-600 bg-opacity-20"
